{
	"name": "rgio-host",
	"version": "0.0.82",
	"description": "Remote Games Hosting App",
	"main": "main.js",
	"scripts": {
		"serve": "electron .",
		"build:dev": "webpack --progress --config webpack.dev.js",
		"build:prod": "webpack --progress --config webpack.prod.js",
		"watch:dev": "webpack --env dev --progress --watch --config webpack.dev.js",
		"watch:prod": "webpack --env prod --progress --watch --config webpack.prod.js",
		"start": "npm-run-all --parallel watch:dev serve",
		"pack:dev": "electron-builder --config ./electron-builder.json --dir",
		"pack:prod": "electron-builder --config ./electron-builder.json --publish always"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/fossephate/rgio-host.git"
	},
